{
  "functions": {
    "app/**/*": {
      "maxDuration": 60
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://sbm-directory.finnydesigns.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "sbm-directory.vercel.app"
        }
      ]
    }
  ]
}